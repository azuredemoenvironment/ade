{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "aliasRegion": {
            "value": "@@aliasRegion@@"
        },
        "defaultPrimaryRegion": {
            "value": "@@defaultPrimaryRegion@@"
        },
        "listOfAllowedLocations": {
            "value": [
                "@@defaultPrimaryRegion@@",
                "@@defaultSecondaryRegion@@"
            ]
        },
        "listOfAllowedSKUs": {
            "value": [
                "Standard_B1ls",
                "Standard_B1ms",
                "Standard_B1s",
                "Standard_B2ms",
                "Standard_B2s",
                "Standard_B4ms",
                "Standard_B4s",
                "Standard_D2s_v3",
                "Standard_D4s_v3"
            ]
        }
    }
}