{
    "githubIssues.issueBranchTitle": "${user}/issue/${issueNumber}-${sanitizedIssueTitle}",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Open Issues",
            "query": "state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "cSpell.words": [
        "Deallocated",
        "Deallocating",
        "Deallocation",
        "Ingestor",
        "Kubernetes",
        "deallocate",
        "deallocates"
    ],
    "cSpell.enableFiletypes": [
        "arm-template",
        "azcli",
        "dockerfile",
        "powershell"
    ],
    "files.watcherExclude": {
        "**/target": true
    },
    "vssolution.altSolutionFolders": [
        "src",
        "app"
    ],
    "azureFunctions.deploySubpath": "app\\ADE.Services\\src\\ADE.EventDispatcher/bin/Release/net5.0/publish",
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~3",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.preDeployTask": "publish (functions)",
}