{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "defaultPrimaryRegion": {
            "value": "@@defaultPrimaryRegion@@"
        },
        "aliasRegion": {
            "value": "@@aliasRegion@@"
        },
        "rootDomainName": {
            "value": "@@rootDomainName@@"
        },
        "appServicePlanResourceGroupName": {
            "value": "@@appServicePlanResourceGroupName@@"
        },
        "adeAppAppServicesResourceGroupName": {
            "value": "@@adeAppAppServicesResourceGroupName@@"
        },
        "adeAppSqlResourceGroupName": {
            "value": "@@adeAppSqlResourceGroupName@@"
        },
        "inspectorGadgetResourceGroupName": {
            "value": "@@inspectorGadgetResourceGroupName@@"
        },
        "containerRegistryResourceGroupName": {
            "value": "@@containerRegistryResourceGroupName@@"
        },
        "adminUserName": {
            "value": "@@adminUsername@@"
        },
        "adminPassword": {
            "reference": {
                "keyVault": {
                    "id": "@@keyVaultResourceID@@"
                },
                "secretName": "resourcePassword"
            }
        }
    }
}