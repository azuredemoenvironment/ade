{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworkGatewayPublicIPAddressName": {
            "value": "@@virtualNetworkGatewayPublicIPAddressName@@"
        },
        "virtualNetwork01Name": {
            "value": "@@virtualNetwork01Name@@"
        },
        "localNetworkGatewayName": {
            "value": "@@localNetworkGatewayName@@"
        },
        "localNetworkGatewayAddressPrefix": {
            "value": "@@localNetworkGatewayAddressPrefix@@"
        },
        "virtualNetworkGatewayName": {
            "value": "@@virtualNetworkGatewayName@@"
        },
        "connectionName": {
            "value": "@@connectionName@@"
        },
        "connectionSharedKey": {
            "reference": {
                "keyVault": {
                    "id": "@@keyVaultResourceID@@"
                },
                "secretName": "resourcePassword"
            }
        },
        "sourceAddressPrefix": {
            "value": "@@sourceAddressPrefix@@"
        },
        "logAnalyticsWorkspaceResourceGroupName": {
            "value": "@@logAnalyticsWorkspaceResourceGroupName@@"
        },
        "logAnalyticsWorkspaceName": {
            "value": "@@logAnalyticsWorkspaceName@@"
        }
    }
}