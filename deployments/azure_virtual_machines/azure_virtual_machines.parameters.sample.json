{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "azureRegion": {
            "value": "@@azureRegion@@"
        },
        "aliasRegion": {
            "value": "@@aliasRegion@@"
        },
        "monitorResourceGroupName": {
            "value": "@@monitorResourceGroupName@@"
        },
        "appConfigResourceGroupName": {
            "value": "@@appConfigResourceGroupName@@"
        },
        "containerRegistryResourceGroupName": {
            "value": "@@containerRegistryResourceGroupName@@"
        },
        "networkingResourceGroupName": {
            "value": "@@networkingResourceGroupName@@"
        },
        "jumpboxResourceGroupName": {
            "value": "@@jumpboxResourceGroupName@@"
        },
        "nTierResourceGroupName": {
            "value": "@@nTierResourceGroupName@@"
        },
        "vmssResourceGroupName": {
            "value": "@@vmssResourceGroupName@@"
        },
        "w10clientResourceGroupName": {
            "value": "@@w10clientResourceGroupName@@"
        },
        "adminUserName": {
            "value": "@@adminUsername@@"
        },
        "adminPassword": {
            "reference": {
                "keyVault": {
                    "id": "@@keyVaultResourceID@@"
                },
                "secretName": "resourcePassword"
            }
        }
    }
}