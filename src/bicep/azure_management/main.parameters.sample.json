{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "allocationStartTime": {
            "value": "@@allocationStartTime@@"
        },
        "appEnvironment": {
            "value": "@@appEnvironment@@"
        },
        "deallocationStartTime": {
            "value": "@@deallocationStartTime@@"
        },
        "environment": {
            "value": "@@environment@@"
        },
        "listOfAllowedLocations": {
            "value": [
                "@@azureRegion@@",
                "@@azurePairedRegion@@"
            ]
        },
        "ownerName": {
            "value": "@@ownerName@@"
        }
    }
}